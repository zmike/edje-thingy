%type doc_comment {const char *}
doc_comment(Y) ::= . {Y = NULL;}
doc_comment(Y) ::= DOC(X).
{
   printf("Comment: %s\n", X->text);
   Y = X->text;
   X->text = NULL;
   edje_parser_token_free(X);
}
